            <kind>1</kind>
            <string>if keyboard_check_pressed(ord('Z'))
    if grid_size &gt; 8
        grid_size /= 2
if keyboard_check_pressed(ord('X'))
    if grid_size &lt; 512
        grid_size *= 2
if keyboard_check_pressed(ord('C'))
{
    if grid_offset = 0
        grid_offset = 1
    else if grid_offset = 1
        grid_offset = 0
}
if keyboard_check_pressed(ord('V'))
{
    if grid_show = 0
        grid_show = 1
    else if grid_show = 1
        grid_show = 0
}
if keyboard_check_pressed(vk_control)
{
    switch (move_speed)
    {
        case 08: move_speed = 16; break;
        case 16: move_speed = 32; break;
        case 32: move_speed = 64; break;
        case 64: move_speed = 08; break;
    }
     
}

if keyboard_check(ord('W'))
    view_yview[0] -= move_speed
if keyboard_check(ord('S'))
    view_yview[0] += move_speed
if keyboard_check(ord('A'))
    view_xview[0] -= move_speed
if keyboard_check(ord('D'))
    view_xview[0] += move_speed
    
if mouse_wheel_down()
{
    if view_scale &lt; 4
    {
        Mx = (mouse_x - view_xview[0]) / view_wview[0]
        My = (mouse_y - view_yview[0]) / view_hview[0]
    
        view_scale += .25
        view_wview[0] = 2560 * view_scale
        view_hview[0] = 1440 * view_scale
        
        view_xview[0] = mouse_x - view_wview[0] * Mx
        view_yview[0] = mouse_y - view_hview[0] * My
    }
}
        
if mouse_wheel_up()
{
    if view_scale &gt; .25
    {
        Mx = (mouse_x - view_xview[0]) / view_wview[0]
        My = (mouse_y - view_yview[0]) / view_hview[0]
        
        view_scale -= .25
        view_wview[0] = 2560 * view_scale
        view_hview[0] = 1440 * view_scale
        
        //var shift_x = camera_get_view_x(view_camera[0]) - (new_w - view_w) * 0.5;
        //var shift_y = camera_get_view_y(view_camera[0]) - (new_h - view_h) * 0.5;
        
        view_xview[0] = mouse_x - view_wview[0] * Mx
        view_yview[0] = mouse_y - view_hview[0] * My
    }
}

if mouse_check_button_pressed(mb_middle)
{
    mouse_move = 1
    mx = mouse_x
    my = mouse_y
}

if mouse_move = 1
{
    if mouse_check_button(mb_middle)
    {
        view_xview[0] = view_xview[0] + mx - mouse_x
        view_yview[0] = view_yview[0] + my - mouse_y
    }

    if mouse_check_button_released(mb_middle)
        mouse_move = 0
}










