            <string>///Check for interupt
//Lasers collision alternate checks every 3rd tick. Collision checking a larger area is faster than running every laser each tick. Accuracy loss is negligable.
if ((inView = 1) and (laserControl.channel = channel)){ 
    endPointCurrent = checkEndPoint(sourceX, sourceY, angle, length + 1) //Return object ID at end point or 0 if none. +1 to account for rounding/floating point errors
    endPointDifference = endPointOld - endPointCurrent 
    
    if (endPointCurrent = 0) //Prevents a scenario where it might take 6 ticks to detect a collision, as hit boxes are only scaled by 3.
        length = maxLength
        
    betweenPoint = checkLine(sourceX, sourceY, angle, length - 1) // -1 to exclude current collision
    
    if ((endPointDifference != 0) or (betweenPoint != 0))
        length = findCollisionPoint(sourceX, sourceY, angle, length)
        
    endPointOld = endPointCurrent
}
